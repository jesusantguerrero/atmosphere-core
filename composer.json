{
    "name": "freesgen/atmosphere",
    "description": "Atmosphere is a SaaS and Starter Kit for Jetstream/Inertia",
    "keywords": ["laravel", "inertia", "jetstream", "SaaS", "Starter Kit"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://atmosphere.neatlancer.com",
    "support": {
        "issues": "https://neatlancer.com/forums",
        "source": "https://neatlancer.com"
    },
    "authors": [
        {
            "name": "Jesus Guerrero",
            "email": "jesusant.guerrero@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freesgen\\Atmosphere\\": "src/"
        }
    },
    "require": {
        "php": "^8.1.0",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/jetstream": "^5.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Freesgen\\Atmosphere\\AtmosphereServiceProvider"
            ]
        }
    }
}
